#!/bin/bash

style=$(cat $HOME/.config/dunst/sid)

if [[ "$style" == "Light" ]]; then
	amixer -Mq set Master,0 5%- unmute && dunstify -u low --replace=69 -i /usr/share/icons/Hybrid_Light/status/24/audio-volume-medium.svg "Volume Down : $(volume)%"
else
	amixer -Mq set Master,0 5%- unmute && dunstify -u low --replace=69 -i /usr/share/icons/Hybrid/status/24/audio-volume-medium.svg "Volume Down : $(volume)%"
fi
